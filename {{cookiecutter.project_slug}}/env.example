# PostgreSQL
POSTGRES_PASSWORD=CHANGEME_POSTGRES_PASSWORD
POSTGRES_USER={{ cookiecutter.project_slug }}

# General settings
DJANGO_ADMIN_URL=aa/
DJANGO_SETTINGS_MODULE=config.settings.production
DJANGO_SECRET_KEY=CHANGEME_SECRET_KEY
DJANGO_ALLOWED_HOSTS=*

# mailgun, see http://cookiecutter-saas.readthedocs.io/en/latest/third_party_services.html#mailgun
DJANGO_MAILGUN_API_KEY=
DJANGO_SERVER_EMAIL={{ cookiecutter.project_slug }} <noreply@mg.{{ cookiecutter.domain_name }}>

# django-allauth
DJANGO_ACCOUNT_ALLOW_REGISTRATION=True

# sentry, see http://cookiecutter-saas.readthedocs.io/en/latest/third_party_services.html#sentry
DJANGO_SENTRY_DSN=

# new relic, see http://cookiecutter-saas.readthedocs.io/en/latest/third_party_services.html#new-relic
NEW_RELIC_LICENSE_KEY=

# stripe, see http://cookiecutter-saas.readthedocs.io/en/latest/third_party_services.html#stripe
PINAX_STRIPE_PUBLIC_KEY=
PINAX_STRIPE_SECRET_KEY=

# octobat, see http://cookiecutter-saas.readthedocs.io/en/latest/third_party_services.html#octobat
OCTOBAT_PRIVATE_KEY=
OCTOBAT_PUBLIC_KEY=

# mailchimp, see http://cookiecutter-saas.readthedocs.io/en/latest/third_party_services.html#mailchimp
MAILCHIMP_API_KEY=
MAILCHIMP_LIST=
